file.rename(filename,gsub(replace,with,filename))
}
dirlist<-list.dirs(path,full.names=TRUE,recursive=FALSE)
if(length(dirlist)) {
for(dirname in dirlist)
recursive_replace(dirname,replace=replace,with=with)
}
}
